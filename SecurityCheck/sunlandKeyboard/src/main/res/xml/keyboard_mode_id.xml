<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="25%" android:horizontalGap="0dp"
    android:verticalGap="0dp" android:keyHeight="10%p">

    <Row>
        <Key android:codes="@integer/KEYBORD_NUMBER_1" android:keyLabel="1"  />
        <Key android:codes="@integer/KEYBORD_NUMBER_2" android:keyLabel="2" />
        <Key android:codes="@integer/KEYBORD_NUMBER_3" android:keyLabel="3" />
        <Key android:codes="@integer/KEYBORD_QUICK1" android:keyLabel="3301"/>
    </Row>
    <Row>
        <Key android:codes="@integer/KEYBORD_NUMBER_4" android:keyLabel="4" />
        <Key android:codes="@integer/KEYBORD_NUMBER_5" android:keyLabel="5" />
        <Key android:codes="@integer/KEYBORD_NUMBER_6" android:keyLabel="6" />
        <Key android:codes="@integer/KEYBORD_QUICK2" android:keyLabel="3302" />
    </Row>
    <Row>
        <Key android:codes="@integer/KEYBORD_NUMBER_7" android:keyLabel="7" />
        <Key android:codes="@integer/KEYBORD_NUMBER_8" android:keyLabel="8" />
        <Key android:codes="@integer/KEYBORD_NUMBER_9" android:keyLabel="9" />
        <Key android:codes="@integer/KEYBORD_QUICK3" android:keyLabel="3303" />
    </Row>
    <Row>
        <Key android:codes="@integer/KEYBORD_ENGLISH_X" android:keyLabel="X" />
        <Key android:codes="@integer/KEYBORD_NUMBER_0" android:keyLabel="0" />
        <Key android:codes="@integer/KEYBORD_DEL" android:keyIcon="@drawable/delete" android:isRepeatable="true" />
        <Key android:codes="@integer/KEYBORD_DONE" android:keyLabel="完成" />
    </Row>
</Keyboard>  
